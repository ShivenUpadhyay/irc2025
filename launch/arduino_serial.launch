<launch>
    <node pkg="rosserial_python" type="serial_node.py" name="arduino_one" output="screen">
        <param name="port" value="/dev/ttyUSB0"/>
        <param name="baud" value="57600"/>
    </node>
    <node pkg="rosserial_python" type="serial_node.py" name="arduino_two" output="screen">
        <param name="port" value="/dev/ttyUSB1"/>
        <param name="baud" value="57600"/>
    </node>
    <node pkg="rosserial_python" type="serial_node.py" name="arduino_three" output="screen">
        <param name="port" value="/dev/ttyUSB2"/>
        <param name="baud" value="57600"/>
    </node>
</launch>